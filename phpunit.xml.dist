<?xml version="1.0"?>
<phpunit
        bootstrap="_tests_bootstrap.php"
        convertErrorsToExceptions="true"
        convertNoticesToExceptions="true"
        convertWarningsToExceptions="true"
        stopOnFailure="true"
        processIsolation="false"
        backupGlobals="false"
        syntaxCheck="true"
        strict="true"
        colors="true"
        >
    <testsuite name="object">
        <directory>./Granam/Strict/Object/Tests</directory>
    </testsuite>
</phpunit>
